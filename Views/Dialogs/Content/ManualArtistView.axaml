<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:suki="clr-namespace:SukiUI.Controls;assembly=SukiUI"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             xmlns:vm="using:MVNFOEditor.ViewModels"
             x:Class="MVNFOEditor.Views.ManualArtistView"
             x:DataType="vm:ManualArtistViewModel">
    <suki:GroupBox Height="150" Width="400" Header="Artist Name">
        <TextBox Height="50" Watermark="Name" Text="{Binding ArtistNameText}"/>
    </suki:GroupBox>
  </UserControl>